{
	"name": "cofi",
	"version": "1.0.0",
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"start": "expo start",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"lint": "npm run lint:ts && npm run lint:js",
		"lint:js": "eslint src App.tsx",
		"lint:js:fix": "eslint src App.tsx --fix",
		"lint:ts": "concurrently \"npm:lint:ts:*\"",
		"lint:ts:ios": "tsc --skipLibCheck --noEmit -p tsconfig.ios.json",
		"lint:ts:android": "tsc --skipLibCheck --noEmit -p tsconfig.android.json",
		"web": "expo start --web"
	},
	"dependencies": {
		"@expo-google-fonts/quicksand": "^0.2.3",
		"@react-native-async-storage/async-storage": "1.21.0",
		"@react-navigation/native": "^6.1.14",
		"@react-navigation/native-stack": "^6.9.22",
		"@segment/sovran-react-native": "^1.1.1",
		"@shopify/restyle": "^2.4.2",
		"@tanstack/react-query": "^5.25.0",
		"expo": "~50.0.11",
		"expo-crypto": "~12.8.1",
		"expo-font": "~11.10.3",
		"expo-localization": "~14.8.3",
		"expo-status-bar": "~1.11.1",
		"i18n-js": "^4.4.3",
		"react": "18.2.0",
		"react-native": "0.73.4",
		"react-native-safe-area-context": "4.8.2",
		"react-native-screens": "~3.29.0",
		"react-native-svg": "14.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@types/react": "~18.2.45",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-config-universe": "^12.0.0",
		"prettier": "^3.2.5",
		"react-native-svg-transformer": "^1.3.0",
		"typescript": "~5.3.0"
	},
	"private": true
}
